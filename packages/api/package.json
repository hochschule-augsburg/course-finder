{
  "name": "api",
  "type": "module",
  "version": "0.0.0",
  "files": [
    "dist/",
    "prisma/"
  ],
  "scripts": {
    "dev:inspect": "tsx watch --inspect --clear-screen=false src/main.ts | pino-pretty",
    "dev": "tsx watch --clear-screen=false src/main.ts | pino-pretty",
    "build": "tsc --noEmit && pnpm run bundle",
    "bundle": "esbuild src/main.ts --bundle --packages=external --format=esm --platform=node --outdir=dist",
    "start": "env NODE_ENV=production node dist/main.js",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "play": "tsx watch playground.ts",
    "test": "vitest run",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate dev",
    "postinstall": "pnpm db:generate||true"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@prisma/adapter-pg": "^5.16.1",
    "@prisma/client": "^5.16.1",
    "@trpc/server": "next",
    "date-fns": "^3.6.0",
    "fastify": "^4.28.1",
    "ldapts": "^7.0.12",
    "lodash-es": "^4.17.21",
    "node-pdftk": "^2.1.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.14",
    "pdf.js-extract": "^0.2.1",
    "pg": "^8.12.0",
    "prisma": "^5.16.1",
    "prisma-json-types-generator": "^3.0.4",
    "superjson": "^2.2.1",
    "totp-generator": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/lodash-es": "^4.17.12",
    "@types/node-pdftk": "^2.1.9",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "esbuild": "^0.22.0",
    "pino-pretty": "^11.2.1",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
