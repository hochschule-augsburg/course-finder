{
  "name": "api",
  "type": "module",
  "version": "0.0.0",
  "files": [
    "dist/",
    "prisma/"
  ],
  "scripts": {
    "dev": "node --experimental-transform-types --watch src/main.ts | pino-pretty",
    "typecheck": "tsc --noEmit",
    "build": "esbuild src/main.ts --bundle --packages=external --format=esm --platform=node --outdir=dist",
    "start": "env NODE_ENV=production TZ=Europe/Berlin node dist/main.js",
    "lint": "eslint --max-warnings 0 src/ prisma/ tests/ --ignore-pattern '**/*.d.ts'",
    "test": "vitest run",
    "db:generate": "prisma generate --no-hints",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --skip-seed --force && prisma db seed",
    "db:migrate": "prisma migrate dev",
    "postinstall": "pnpm run db:generate"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.0.3",
    "@google/generative-ai": "^0.24.1",
    "@prisma/adapter-pg": "^6.14.0",
    "@prisma/client": "^6.14.0",
    "@trpc/server": "^11.5.0",
    "date-fns": "^4.1.0",
    "fastify": "^5.5.0",
    "ldapts": "^8.0.9",
    "lodash-es": "^4.17.21",
    "node-pdftk": "^2.1.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.5",
    "pdf.js-extract": "^0.2.1",
    "pg": "^8.16.3",
    "prisma": "^6.14.0",
    "prisma-json-types-generator": "^3.6.0",
    "superjson": "^2.2.2",
    "totp-generator": "^1.0.0",
    "@e965/xlsx": "^0.20.3",
    "zod": "^3",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/lodash-es": "^4.17.12",
    "@types/node-pdftk": "^2.1.9",
    "@types/node-schedule": "^2.1.8",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "esbuild": "^0.25.9",
    "pino-pretty": "^13.1.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "prisma": {
    "seed": "node prisma/seed.ts"
  }
}
