{
  "name": "api",
  "type": "module",
  "version": "0.0.0",
  "files": [
    "dist/",
    "prisma/"
  ],
  "scripts": {
    "dev:inspect": "docker start postgres-sub-enroll & tsx watch --inspect --clear-screen=false src/main.ts | pino-pretty",
    "dev": "docker start postgres-sub-enroll & tsx watch --clear-screen=false src/main.ts | pino-pretty",
    "build": "tsc --noEmit && pnpm run bundle",
    "bundle": "esbuild src/main.ts --bundle --packages=external --format=esm --platform=node --outdir=dist",
    "start": "env NODE_ENV=production node dist/main.js",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "play": "tsx watch playground.ts",
    "test": "vitest run",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate dev reset",
    "db:migrate": "prisma migrate dev",
    "postinstall": "pnpm db:generate||true"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/session": "^10.7.2",
    "@prisma/adapter-pg": "^5.13.0",
    "@prisma/client": "^5.14.0",
    "@trpc/server": "next",
    "date-fns": "^3.6.0",
    "fastify": "^4.13.0",
    "ldapts": "^7.0.11",
    "lodash-es": "^4.17.21",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "prisma": "^5.13.0",
    "prisma-json-types-generator": "^3.0.4",
    "sendmail": "^1.6.1",
    "superjson": "^2.2.1",
    "totp-generator": "^1.0.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@trpc/client": "next",
    "@types/cors": "^2.8.17",
    "@types/lodash-es": "^4.17.12",
    "@types/node-cron": "^3.0.11",
    "@types/node-schedule": "^2.1.7",
    "@types/pg": "^8.11.5",
    "@types/sendmail": "^1.4.7",
    "esbuild": "^0.21.2",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2",
    "vitest-mock-extended": "^1.3.1"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
